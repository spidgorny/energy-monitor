{
    "require": {
        "google/apiclient": "2.0",
        "nao-pon/flysystem-google-drive": "~1.1",
        "symfony/yaml": "3.4.x-dev",
        "spidgorny/nadlib": "dev-default"
    },
	"require-bak": {
		"spidgorny/nadlib": "dev-slawa"
	},
	"autoload": {
		"classmap": [
			"vendor/spidgorny/nadlib",
			"src/class"
		],
